{"ast":null,"code":"import Header from './components/Header';\nimport Tasks from './components/Tasks';\nimport AddTask from './components/AddTask';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false\n    };\n  },\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask;\n    },\n    addTask(task) {\n      this.tasks = [...this.tasks, task];\n    },\n    deleteTask(id) {\n      if (confirm('Are you sure?')) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n    toggleReminder(id) {\n      this.tasks = this.tasks.map(task => task.id === id ? {\n        ...task,\n        reminder: !task.reminder\n      } : task);\n    },\n    async fetchTasks() {\n      const res = await fetch('http://localhost:5000/tasks');\n      const data = await res.json();\n      return data;\n    }\n  },\n  async created() {\n    this.tasks = await this.fetchTasks();\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,KAAI,MAAQ,oBAAmB;AACtC,OAAOC,OAAM,MAAQ,sBAAqB;AAE1C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,MAAM;IACNC,KAAK;IACLC;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,IAAI,CAACF,WAAU,GAAI,CAAC,IAAI,CAACA,WAAU;IACrC,CAAC;IACDG,OAAO,CAACC,IAAI,EAAC;MACX,IAAI,CAACL,KAAI,GAAI,CAAC,GAAG,IAAI,CAACA,KAAK,EAAEK,IAAI;IACnC,CAAC;IACDC,UAAU,CAACC,EAAE,EAAC;MACZ,IAAIC,OAAO,CAAC,eAAe,CAAC,EAAC;QAC3B,IAAI,CAACR,KAAI,GAAI,IAAI,CAACA,KAAK,CAACS,MAAM,CAAEJ,IAAI,IAAKA,IAAI,CAACE,EAAC,KAAMA,EAAE;MACzD;IACF,CAAC;IACDG,cAAc,CAACH,EAAE,EAAC;MAChB,IAAI,CAACP,KAAI,GAAI,IAAI,CAACA,KAAK,CAACW,GAAG,CAAEN,IAAI,IAAKA,IAAI,CAACE,EAAC,KAAMA,EAAC,GAAI;QAAC,GAAGF,IAAI;QAAEO,QAAQ,EAAE,CAACP,IAAI,CAACO;MAAQ,IAAIP,IAAI;IACnG,CAAC;IACD,MAAMQ,UAAU,GAAG;MACjB,MAAMC,GAAE,GAAI,MAAMC,KAAK,CAAC,6BAA6B;MAErD,MAAMhB,IAAG,GAAI,MAAMe,GAAG,CAACE,IAAI,EAAC;MAE5B,OAAOjB,IAAG;IACZ;EACF,CAAC;EACD,MAAMkB,OAAO,GAAG;IACd,IAAI,CAACjB,KAAI,GAAI,MAAM,IAAI,CAACa,UAAU,EAAC;EACrC;AACF","names":["Header","Tasks","AddTask","name","components","data","tasks","showAddTask","methods","toggleAddTask","addTask","task","deleteTask","id","confirm","filter","toggleReminder","map","reminder","fetchTasks","res","fetch","json","created"],"sourceRoot":"","sources":["/Users/rorymartin/Documents/repos/vue-practice/vue-crash-2021/src/App.vue"],"sourcesContent":["<!-- npm run serve -->\n\n<template>\n  <div class=\"container\">\n    <Header @toggle-add-task=\"toggleAddTask\" title=\"Task Tracker\" :showAddTask=\"showAddTask\" />\n    <div v-show=\"showAddTask\" >\n      <AddTask @add-task=\"addTask\"/>\n    </div>\n    <Tasks @toggle-reminder=\"toggleReminder\" @delete-task=\"deleteTask\" :tasks=\"tasks\" />\n  </div>\n</template>\n\n<script>\nimport Header from './components/Header'\nimport Tasks from  './components/Tasks'\nimport AddTask from  './components/AddTask'\n\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false,\n    }\n  },\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask\n    },\n    addTask(task){\n      this.tasks = [...this.tasks, task]\n    },\n    deleteTask(id){\n      if (confirm('Are you sure?')){\n        this.tasks = this.tasks.filter((task) => task.id !== id)\n      }\n    },\n    toggleReminder(id){\n      this.tasks = this.tasks.map((task) => task.id === id ? {...task, reminder: !task.reminder} : task)\n    },\n    async fetchTasks() {\n      const res = await fetch('http://localhost:5000/tasks')\n\n      const data = await res.json()\n\n      return data\n    }\n  },\n  async created() {\n    this.tasks = await this.fetchTasks()\n  },\n}\n</script>\n\n<style>\n  @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap');\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: 'Poppins', sans-serif;\n}\n\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n\n.btn:focus {\n  outline: none;\n}\n\n.btn:active {\n  transform: scale(0.98);\n}\n\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}