{"ast":null,"code":"import Header from './components/Header';\nimport Tasks from './components/Tasks';\nimport AddTask from './components/AddTask';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n  data() {\n    return {\n      tasks: [],\n      showAddTask: false\n    };\n  },\n  methods: {\n    toggleAddTask() {\n      this.showAddTask = !this.showAddTask;\n    },\n    addTask(task) {\n      this.tasks = [...this.tasks, task];\n    },\n    deleteTask(id) {\n      if (confirm('Are you sure?')) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n    toggleReminder(id) {\n      this.tasks = this.tasks.map(task => task.id === id ? {\n        ...task,\n        reminder: !task.reminder\n      } : task);\n    }\n  },\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: \"Doctors Appointment\",\n      day: \"March 1st at 2.30pm\",\n      reminder: true\n    }, {\n      id: 2,\n      text: \"Workout\",\n      day: \"March 4stat 09.00am\",\n      reminder: true\n    }, {\n      id: 3,\n      text: \"Swimming Appointment\",\n      day: \"March 1st at 2.30pm\",\n      reminder: false\n    }];\n  }\n};","map":{"version":3,"mappings":"AAaA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,KAAI,MAAQ,oBAAmB;AACtC,OAAOC,OAAM,MAAQ,sBAAqB;AAE1C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,MAAM;IACNC,KAAK;IACLC;EACF,CAAC;EACDG,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,IAAI,CAACF,WAAU,GAAI,CAAC,IAAI,CAACA,WAAU;IACrC,CAAC;IACDG,OAAO,CAACC,IAAI,EAAC;MACX,IAAI,CAACL,KAAI,GAAI,CAAC,GAAG,IAAI,CAACA,KAAK,EAAEK,IAAI;IACnC,CAAC;IACDC,UAAU,CAACC,EAAE,EAAC;MACZ,IAAIC,OAAO,CAAC,eAAe,CAAC,EAAC;QAC3B,IAAI,CAACR,KAAI,GAAI,IAAI,CAACA,KAAK,CAACS,MAAM,CAAEJ,IAAI,IAAKA,IAAI,CAACE,EAAC,KAAMA,EAAE;MACzD;IACF,CAAC;IACDG,cAAc,CAACH,EAAE,EAAC;MAChB,IAAI,CAACP,KAAI,GAAI,IAAI,CAACA,KAAK,CAACW,GAAG,CAAEN,IAAI,IAAKA,IAAI,CAACE,EAAC,KAAMA,EAAC,GAAI;QAAC,GAAGF,IAAI;QAAE